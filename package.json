{
  "private": true,
  "scripts": {
    "build:file": "node build/bin/build-entry.js",
    "build:theme": "node build/bin/gen-cssfile",
    "dev": " npm run build:file & webpack-dev-server --config build/webpack.demo.js",
    "build": "webpack --env.prod",
    "test": "jest  --runInBand test",
    "lint": "prettier --write  \"packages/**/*.?(ts|vue)\"",
    "coverage": "jest --coverage"
  },
  "version": "v0.0.0-alpha.0",
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node build/bin/verifyCommit.js"
  },
  "lint-staged": {
    "*.js,*.json": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "git add"
    ]
  },
  "dependencies": {
    "throttle-debounce": "^2.2.1",
    "vue": "3.0.0-beta.20"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/jest": "^25.2.1",
    "@types/mocha": "^7.0.2",
    "@vue/compiler-sfc": "3.0.0-beta.20",
    "@vue/test-utils": "2.0.0-alpha.6",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "json-templater": "^1.2.0",
    "lint-staged": "^10.1.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "ts-jest": "^25.3.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.0",
    "vue-jest": "^5.0.0-alpha.0",
    "vue-loader": "^16.0.0-alpha.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yorkie": "^2.0.0"
  }
}
